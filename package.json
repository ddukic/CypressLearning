{
  "scripts": {
    "cypress:run": "cypress run --env passed=$CYPRESS_PASSED,failed=$CYPRESS_FAILED",
    "format": "npx prettier --write .",
    "cypress:open": "cypress open"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "cypress": "^11.2.0",
    "husky": "^8.0.2",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3"
  }
}

